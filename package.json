{
  "name": "packmovego_desktop",
  "version": "0.1.0",
  "type": "module",
  "description": "PackMoveGo Desktop Application - Moving and Packing Services",
  "main": "config/server.js",
  "scripts": {
    "dev": "vite --config config/vite.config.csr.js",
    "dev:secure": "node scripts/dev-server.js",
    "certs:generate": "chmod +x scripts/generate-certs.sh && ./scripts/generate-certs.sh",
    "dev:ssr": "npm run build:ssr && npm run preview:ssr",
    "build": "npm run build:csr",
    "build:csr": "vite build --config config/vite.config.csr.js",
    "build:ssr": "vite build --config config/vite.config.ssr.js",
    "build:both": "npm run build:csr && npm run build:ssr",
    "build:vercel": "npm run build:both",
    "build:clean": "rm -rf dist && npm run build:both",
    "build:analyze": "vite build --config config/vite.config.csr.js --mode analyze",
    "build:profile": "vite build --config config/vite.config.csr.js --mode profile",
    "preview": "npm run preview:csr",
    "preview:csr": "vite build --config config/vite.config.csr.js && vite preview --config config/vite.config.csr.js --port 5001",
    "preview:ssr": "NODE_ENV=production node scripts/vite-ssr-preview.js",
    "preview:existing": "NODE_ENV=production node scripts/vite-ssr-preview.js",
    "preview:static": "npx serve dist -p 5050",
    "preview:build": "npm run build:both && npm run preview:ssr",
    "start": "npm run build:ssr && NODE_ENV=production node config/server.js",
    "server": "NODE_ENV=production node config/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --max-warnings 0",
    "lint:fix": "eslint src --fix",
    "type-check": "tsc --noEmit --project config/tsconfig.json",
    "perf:lighthouse": "npx lighthouse http://localhost:5002 --output html --output-path ./lighthouse-report.html",
    "deploy": "npm run build:both && vercel --prod",
    "deploy:ssr": "npm run build:both && vercel --prod --yes",
    "dev:api:enable": "node scripts/dev-api-toggle.js enable",
    "dev:api:disable": "node scripts/dev-api-toggle.js disable",
    "dev:api:status": "node scripts/dev-api-toggle.js status"
  },
  "dependencies": {
    "@babel/plugin-transform-react-constant-elements": "^7.27.1",
    "@babel/plugin-transform-react-inline-elements": "^7.27.1",
    "@hookform/resolvers": "^5.0.1",
    "@tanstack/react-query": "^5.75.5",
    "@types/cors": "^2.8.18",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "npm": "^11.4.2",
    "react": "^18.2.0",
    "react-calendar": "^5.1.0",
    "react-dom": "^18.2.0",
    "react-ga4": "^2.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.56.2",
    "react-router-dom": "^6.30.0",
    "update": "^0.4.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@types/express": "^4.17.23",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.15.21",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "concurrently": "^8.2.2",
    "cssnano": "^7.0.7",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.20",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.35",
    "postcss-nested": "^7.0.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.43.1",
    "ts-jest": "^29.4.0",
    "vite": "^6.3.5"
  },
  "engines": {
    "node": ">=18.14.0",
    "npm": ">=8.0.0"
  },
  "funding": {
    "type": "individual",
    "url": "http://packmovego.com/donate"
  },
  "keywords": [
    "moving",
    "packing",
    "services",
    "react",
    "vite",
    "typescript",
    "ssr"
  ],
  "author": "PackMoveGo Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/packmovego/desktop.git"
  },
  "bugs": {
    "url": "https://github.com/packmovego/desktop/issues"
  },
  "homepage": "https://packmovego.com"
}
